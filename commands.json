{
    "$schema": "https://cdn.subsquid.io/schemas/commands.json",
    "commands": {
      "archive-up": {
        "description": "Start local Selendra Archive",
        "cmd": ["docker-compose", "-f", "explorer/archive/docker-compose.archive.yml", "up", "-d"]
      },
      "archive-down": {
        "description": "Stop local Selendra Archive",
        "cmd": ["docker-compose", "-f", "explorer/archive/docker-compose.archive.yml", "down"]
      },
      "up": {
        "description": "Start a PG database",
        "cmd": ["docker-compose", "-f", "explorer/docker-compose.yml", "up", "-d"]
      },
      "down": {
        "description": "Drop a PG database",
        "cmd": ["docker-compose", "-f", "explorer/docker-compose.yml", "down"]
      }
    }
  }
  